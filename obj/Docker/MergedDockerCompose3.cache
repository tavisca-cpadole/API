services:
  testapi2:
    build:
      context: C:\Users\cpadole\source\repos\TestAPI2
      dockerfile: TestAPI2/Dockerfile
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTPS_PORT: '44396'
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: testapi2
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  TestAPI2.dll'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/bash -c "if PID=$$(pidof
        -x dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 54411
      target: 80
    - published: 52509
      target: 80
    - published: 44396
      target: 443
    volumes:
    - C:\Users\cpadole\vsdbg\vs2017u5:/remote_debugger:ro
    - C:\Users\cpadole\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\cpadole\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
version: '3.4'